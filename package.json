{
  "name": "bigbrother",
  "version": "1.0.0",
  "description": "a simple and flexible form valiation lib for React and plain JavaScript apps",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:watch",
    "dev:watch": "webpack --config webpack.dev.config -w",
    "build": "webpack --config webpack.prod.config.js",
    "demo:react": "webpack --config webpack.demo.config.js"
  },
  "readmeFilename": "README.md",
